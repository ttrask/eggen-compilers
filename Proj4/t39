}

void monkey(void)
{                        /* good */
   int q;    
   void a[4];
   int b;
   void c;

   b=b/2;
   while (b==3)
   {
       int x;
       x=q+2;
   }
   if (c==b)
   {
   
       a = 3;
       c=car(4);
   }
}

